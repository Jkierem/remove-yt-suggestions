{
    "name": "Remove Youtube Suggestions",
    "version": "1.0",
    "description": "Remove Youtube Suggestions",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_idle",
            "js": ["content.js"]
        }
    ]
}